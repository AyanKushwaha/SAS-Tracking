<?xml version="1.0" encoding="UTF-8" ?>
<!-- 
  Default for Crew Portal. Override with system specific host configuration 
-->

<bids>
    <bids_user>${CARMUSR}/bids_user</bids_user>
    <maven_home>${CARMUSR}/lib/maven/apache-maven-3.2.3</maven_home>
    <!-- <java_home>/usr/lib/jvm/java-1.7.0</java_home> -->
    <java_home>/etc/alternatives/jre_1.8.0</java_home>
    <jboss_home>/usr/java/jboss-eap</jboss_home>
    <jboss_home_deploy>%{jboss_home}/standalone/deployments</jboss_home_deploy>
    <jboss_home_conf>%{jboss_home}/standalone/crewportal</jboss_home_conf>
    <app_home>${CARMTMP}/crewportal</app_home>
    <app_home_deploy>%{app_home}/deploy/*</app_home_deploy>
    <app_home_conf>%{app_home}/conf.app/*</app_home_conf>    
    <deploy_env>${CARMTMP}/compile/deploy_env.properties</deploy_env>
</bids>
