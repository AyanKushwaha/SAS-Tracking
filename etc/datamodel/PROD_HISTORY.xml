<?xml version="1.0" encoding="UTF-8" ?>
<!-- Data model configuration -->
<data_model>
   <period_start>now+0</period_start>
   <period_end>now_extended-1</period_end> <!-- bugzilla 25367, sys calculates end date -->
   <extend_date>16</extend_date>
   <schema>%(db/schema_history)</schema>
   <plan_dir>Database/History</plan_dir>
   <plan_path>%(data_model/plan_dir)/%(db/schema_history)/%(db/schema_history)</plan_path>
   <interface name="db_node" url="%(db/dave/history_readonly/url)"/>
</data_model>
