<?xml version="1.0" standalone="no" ?>

<!DOCTYPE CustomAttributes 
  SYSTEM "../../current_carmsys_cct/data/config/CustomAttributes.dtd">

<CustomAttributes>

<Entity name="leg">

<!-- off-block and in-block times -->
<!-- Estimated off-block time -->
<Attribute name="eobt" type="abstime"/> 
<!-- Estimated in-block time -->
<Attribute name="eibt" type="abstime"/> 
<!-- Actual off-block time -->
<Attribute name="aobt" type="abstime"/> 
<!-- Actual in-block time -->
<Attribute name="aibt" type="abstime"/> 
<!-- Actual take-off time -->
<Attribute name="atot" type="abstime" table="flight_leg"/>
<!-- Original suffix -->
<Attribute name="origsuffix" type="string" table="flight_leg"/>

</Entity>

</CustomAttributes>
