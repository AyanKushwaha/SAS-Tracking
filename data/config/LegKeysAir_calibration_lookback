1F:LEG_KEY_SUFFIX
1F:LEG_KEY_LDOP
1F:LEG_KEY_DEPARTURE_TIME
1F:LEG_KEY_ARRIVAL_TIME
1F:LEG_KEY_DEPARTURE_AIRP
1F:LEG_KEY_ARRIVAL_AIRP

2F:LEG_KEY_DUTY_DES
2F:LEG_KEY_CARRIER
2F:LEG_KEY_FLIGHT_NUMBER
2F:LEG_KEY_SUFFIX
2F:LEG_KEY_LDOP
2F:LEG_KEY_DEPARTURE_TIME
2F:LEG_KEY_ARRIVAL_TIME
2F:LEG_KEY_DEPARTURE_AIRP
2F:LEG_KEY_ARRIVAL_AIRP
