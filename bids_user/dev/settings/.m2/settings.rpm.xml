      <profile>
        <id>jeppesen</id>
        <repositories>
          <repository>
            <id>jeppesen_repo</id>
            <name>Repository for Jeppesen builds</name>
            <!--<url>file:///opt/jeppesen/jcms/dist/crewweb/m2/repository</url>-->
            <url>file:///c:/tmp/opt/jeppesen/jcms/dist/crewweb/m2/repository</url>
            <layout>default</layout>
          </repository>
        </repositories>
      </profile>
  </profiles>

  <activeProfiles>
    <activeProfile>jeppesen</activeProfile>
  </activeProfiles>
</settings>
