<?xml version="1.0" encoding="UTF-8"?>
<bid
	type="vacation" 
	overview_category="off"
    xmlns="http://carmen.jeppesen.com/crewweb/jmpframework/xmlschema/bidtype"
    xmlns:common="http://carmen.jeppesen.com/crewweb/framework/xmlschema/common" 
    xmlns:fw="http://carmen.jeppesen.com/crewweb/framework/xmlschema/bidtype">

  <fw:attribute name="help" value="texthelp-section" />

	<fw:properties>
		<fw:head></fw:head>
		<fw:body layout="simple">
		      <fw:property file="ValidityPeriodLeaveAlt1.xml"/>
		      <fw:property file="ValidityPeriodLeaveAlt2.xml"/>
		      <fw:property file="ValidityPeriodLeaveAlt3.xml"/>
		</fw:body>
	</fw:properties>
	
	<fw:presentation class="com.jeppesen.carmen.crewweb.jmpframework.customization.standard.SimpleBidDetailsFormatter" >
    	<fw:width px="350"/>
        <fw:height px="200"/>
	</fw:presentation>
	
	 <jmpBidToBidBidMapping>
        <property type="validity_period_alt_1">
            <alternative index="1">
                <jmpBidEntry jmpEntryName="start" bidEntryName="start" />
                <jmpBidEntry jmpEntryName="end" bidEntryName="end" />
            </alternative>
        </property>
        <property type="validity_period_alt_2">
            <alternative index="1">
                <jmpBidEntry jmpEntryName="start" bidEntryName="start" />
                <jmpBidEntry jmpEntryName="end" bidEntryName="end" />
            </alternative>
        </property>
        <property type="validity_period_alt_3">
            <alternative index="1">
                <jmpBidEntry jmpEntryName="start" bidEntryName="start" />
                <jmpBidEntry jmpEntryName="end" bidEntryName="end" />
            </alternative>
        </property>
        
    </jmpBidToBidBidMapping>
	
</bid>
