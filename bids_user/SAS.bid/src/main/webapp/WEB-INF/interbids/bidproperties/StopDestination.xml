<?xml version="1.0" encoding="UTF-8"?>
<property
    widget_type="ComboBox"
    type="stop_destination"
	xmlns="http://carmen.jeppesen.com/crewweb/interbids/xmlschema/bidproperty"
	xmlns:fw="http://carmen.jeppesen.com/crewweb/framework/xmlschema/bidproperty">
    <fw:attributes>
        <fw:attribute name="name" value="stop_destination"/>
        <fw:attribute name="allowBlank" value="false" />
        <fw:attribute name="emptyText" value="-Select-"/>
        <fw:attribute name="valueField" value="value" /><!-- the data sent to server when saving -->
        <fw:attribute name="displayField" value="name" /><!-- the data displayed in the drop down -->
        <fw:attribute name="allowNonStoreData" value="false"/>
    </fw:attributes>
    
    <fw:data_source name="combo" ref_id="etab.stop_destination"/>
    
<!--     <filter class="" /> -->
    
</property>
