module fundamental_cct_fc inherits fundamental_cct

redefine global export %flight_crew% = true;
redefine global export %cabin_crew% = false;

redefine export %main_cat% = "F";

redefine export %upper_assigned_vector_pos% = 1;
redefine export %lower_assigned_vector_pos% = 2;
redefine export %upper_cat% = "FC";
redefine export %lower_cat% = "FP";
