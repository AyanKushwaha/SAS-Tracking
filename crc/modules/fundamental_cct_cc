module fundamental_cct_cc inherits fundamental_cct

redefine global export %flight_crew% = false;
redefine global export %cabin_crew% = true;

redefine export %main_cat% = "C";

redefine export %upper_assigned_vector_pos% = 5;
redefine export %lower_assigned_vector_pos% = 7;
redefine export %upper_cat% = "AP";
redefine export %lower_cat% = "AH";
