# Carm variables need to be set for the tests to work properly

def main(as_main=False, args=None):
    ret = []
    ret.append('os.environ["CARMSYSTEM"]="<carmsystemroot>"')
    ret.append('os.environ["CARMSYSTEMROOT"]="<carmsystemroot>"')
    ret.append('os.environ["CARMSYS"]="<carmsystemroot>/carmsys"')
    ret.append('os.environ["CARMUSR"]="<carmsystemroot>/<carmusr>"')
    ret.append('os.environ["CARMDATA"]="<carmsystemroot>/carmdata_behave"')
    ret.append('os.environ["CARMTMP"]="<carmsystemroot>/carmtmp_behave"')

    return ret
